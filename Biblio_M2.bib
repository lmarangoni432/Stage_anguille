@article{thieu_nutrient_2009,
	title = {Nutrient transfer in three contrasting {NW} {European} watersheds: {The} {Seine}, {Somme}, and {Scheldt} {Rivers}. {A} comparative application of the {Seneque}/{Riverstrahler} model},
	volume = {43},
	issn = {0043-1354},
	shorttitle = {Nutrient transfer in three contrasting {NW} {European} watersheds},
	url = {https://www.sciencedirect.com/science/article/pii/S0043135409000372},
	doi = {10.1016/j.watres.2009.01.014},
	abstract = {An understanding of the ecological functioning of an aquatic continuum on a multi-regional scale relies on the ability to collect suitable descriptive information. Here, the deterministic Seneque/Riverstrahler model, linking biogeochemistry with the constraints set by geomorphology and anthropogenic activities, was fully implemented to study the Seine, Somme, and Scheldt Rivers. Reasonable agreement was found between calculated and observed nutrient fluxes for both seasonal and inter-annual variations along the networks. Nutrient budgets underline: i) a clear partition of diffuse and point sources with respect to the specific activities of the watersheds, ii) the importance of riparian retention, responsible for 25–50\% of nitrogen retention, iii) the role played by benthic processes, resulting in the retention of up to 45\% of the phosphorus and 35\% of the silica entering the river systems. Nutrient ratios confirmed that fluxes to the Eastern Southern Bight of the North Sea are imbalanced, supporting the potential for undesirable algal blooms.},
	number = {6},
	urldate = {2024-09-16},
	journal = {Water Research},
	author = {Thieu, Vincent and Billen, Gilles and Garnier, Josette},
	month = apr,
	year = {2009},
	keywords = {Biogeochemical modeling, Nitrogen, Nutrient budget, Phosphorus, Scheldt River, Seine River, Silica, Somme River},
