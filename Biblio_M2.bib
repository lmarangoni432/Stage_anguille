	pmid = {9267396},
	keywords = {Animals, Female, Fish Diseases, Fresh Water, Helminthiasis, Helminthiasis, Animal, Killifishes, Male, Nebraska, Prevalence, Protozoan Infections, Protozoan Infections, Animal, Species Specificity},
	pages = {584--592},
