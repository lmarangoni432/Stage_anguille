}

@phdthesis{adam_anguille_1997,
	type = {phdthesis},
	title = {L'anguille européenne ({Anguilla} anguilla {L}. 1758) : dynamique de la sous-population du {Lac} de {Grand} {Lieu} en relation avec les facteurs environnementaux et anthropiques},
	shorttitle = {L'anguille européenne ({Anguilla} anguilla {L}. 1758)},
	url = {https://hal.inrae.fr/tel-02575906},
	abstract = {L'approche de la dynamique de la population d'anguilles européennes est réduite à la fraction continentale du cycle biologique sur le lac de Grand Lieu. Un carnet de pêche journalier est instauré depuis 1990 auprès de la pêcherie. Sur un an, un double échantillonnage est réalisé sur les captures professionnelles d'une part, et au moyen d'engins expérimentaux d'autre part. L'estimation de l'âge par otolithométrie d'un échantillon d'individus et la modélisation de la croissance par sexe sont effectuées. L'exploitation historique et récente des anguilles a été analysée. Un modèle structural permet d'estimer les abondances absolues aux âges et les taux instantanés de mortalité par pêche aux âges. Les soutiens d'effectifs effectués par les pêcheurs professionnels, les pressions halieutiques et la prédation aviaire sont les principaux facteurs agissant sur la dynamique de la sous-population d'anguilles du lac de Grand Lieu .},
	language = {fr},
	urldate = {2024-09-16},
	school = {Doctorat Hydrobiologie, Université Paul Sabatier Toulouse III},
	author = {Adam, G.},
	year = {1997},
	note = {Pages: 353},
	file = {Snapshot:/Users/lucasmarangoni/Zotero/storage/DUY4LK7Z/tel-02575906.html:text/html},
}

@book{rocher_evolution_2017,
	address = {Paris},
	title = {Évolution de la qualité de la {Seine} en lien avec les progrès de l'assainissement de 1970 à 2015},
	isbn = {979-10-91089-31-9},
	abstract = {Quelle est la qualité de la Seine aujourd'hui ? Les activités humaines croissantes en agglomération parisienne l'ont-elles dégradée ? Cet ouvrage répond à ces questions récurrentes à une époque où les attentes vis-à-vis de ce fleuve sont importantes, notamment pour y promouvoir les activités récréatives. Ce livre s'intéresse à l'évolution de la qualité du fleuve depuis le début du siècle dernier. Le lien entre les grandes étapes du déploiement de l'assainissement francilien et l'évolution de la qualité de la Seine est mis en lumière. A la frontière entre récit historique et ouvrage technique, ce livre propose un voyage de plus d'un siècle, qui débute à l'époque où les eaux d'égout étaient épandues sur les champs agricoles et la Seine asphyxiée, pour finir de nos jours où un système d'assainissement performant protège la Seine et préserve sa qualité},
	language = {fr},
	publisher = {Éditions Johanet},
